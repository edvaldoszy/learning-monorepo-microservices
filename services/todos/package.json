    "test": "jest --detectOpenHandles --forceExit"
  },
  "dependencies": {
    "@monorepo/helpers": "1.0.0",
    "@tinyhttp/app": "1.3.15",
    "@tinyhttp/cors": "1.3.2",
    "@tinyhttp/dotenv": "1.3.0",
    "@tinyhttp/logger": "1.3.0",
    "@tinyhttp/router": "1.3.3",
    "agenda": "4.2.1",
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.0",
    "date-fns": "2.23.0",
    "jsonwebtoken": "8.5.1",
    "mongodb": "4.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "7.14.8",
    "@babel/node": "7.14.7",
    "@babel/preset-env": "7.14.8",
    "@babel/preset-typescript": "7.14.5",
    "@types/agenda": "4.1.0",
    "@types/bcryptjs": "2.4.2",
    "@types/body-parser": "1.19.1",
    "@types/faker": "5.5.7",
    "@types/jsonwebtoken": "8.5.4",
    "@types/mongodb": "4.0.7",
    "@types/node": "14.17.5",
    "@types/supertest": "2.0.11",
    "babel-plugin-module-resolver": "4.1.0",
    "faker": "5.5.3",
    "jest": "27.0.6",
    "nodemon": "2.0.12",
    "supertest": "6.1.4",
    "typescript": "4.3.5"
  }
}
