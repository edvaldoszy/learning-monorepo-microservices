{
  "name": "@monorepo/customers-service",
  "version": "0.1.0",
  "scripts": {
    "build": "babel src --out-dir dist --delete-dir-on-start --extensions '.js,.ts' --ignore '**/*.spec.ts,**/*.test.ts' --copy-files --no-copy-ignored",
    "start": "nodemon --ext 'js,ts' --exec babel-node --extensions '.js,.ts' src/index.ts",
    "test": "jest"
  },
  "dependencies": {
    "@tinyhttp/app": "1.3.15",
    "@tinyhttp/cors": "1.3.2",
    "@tinyhttp/dotenv": "1.3.0",
    "@tinyhttp/logger": "1.3.0",
    "@tinyhttp/router": "1.3.3"
  },
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "7.14.8",
    "@babel/node": "7.14.7",
    "@babel/preset-env": "7.14.8",
    "@babel/preset-typescript": "7.14.5",
    "babel-plugin-module-resolver": "4.1.0",
    "jest": "27.0.6",
    "nodemon": "2.0.12",
    "typescript": "4.3.5"
  }
}
